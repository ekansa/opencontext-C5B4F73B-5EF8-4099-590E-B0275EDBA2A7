          <oc:var_label type="alphanumeric">Type Description</oc:var_label>
          <oc:show_val>Simple, blown, transparent, colorless spheroid with snapped ends and hollow body similar to a christmas ornament.</oc:show_val>
        </arch:property>
        <arch:property>
          <arch:variableID>9B98B318-F9EB-4D5D-2AB4-4C03735B607F</arch:variableID>
